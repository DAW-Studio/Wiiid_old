  "hold": {
    "a": { "device":"keyboard", "type":"cycle", "mod":["","",""], "key":["a","b","c"], "release":true, "cycle":0 }
  },
  "hold+tap": {
    "b,a": { "device":"keyboard", "type":"standard", "mod":"", "key":"a", "release":true }
  },
  "tilt": {
    "-z": {"device":"mouse", "type":"position_relative", "x":-10, "y":0},
    "+z": {"device":"mouse", "type":"position_relative", "x":10, "y":0}
  }
