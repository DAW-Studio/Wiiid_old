{
  "tap": {
    "up": ["", "up", true, 0],
    "down": ["", "down", true, 0],
    "left": ["gui","z", true, 0],
    "right": ["gui+shift", "z", true, 0],
    "plus": ["", "rightbracket", true, 0],
    "minus": { "device":null, "type":"function", "func":"restart", "args":[] },
    "a": { "device":"keyboard", "type":"cycle", "mod":["","",""], "key":["a","b","c"], "release":true, "cycle":0 }
